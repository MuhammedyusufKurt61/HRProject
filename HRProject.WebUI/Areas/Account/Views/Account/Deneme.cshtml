@model PersonelLoginVM
@{
    ViewData["Title"] = "Deneme";
    Layout = "~/Areas/Account/Views/Shared/_DenemeLayout.cshtml";
}


<!-- partial:index.partial.html -->
<div class="cont">
    <div class="demo">
        <div class="login">
            <form asp-action="Deneme" asp-area="Account" asp-controller="Account">
                <div class="login__check"></div>
                <div class="login__form">
                    <div class="login__row">
                        <svg class="login__icon name svg-icon" viewBox="0 0 20 20">
                            <path d="M0,20 a10,8 0 0,1 20,0z M10,0 a4,4 0 0,1 0,8 a4,4 0 0,1 0,-8" />
                        </svg>
                        <input asp-for="Email" type="text" class="login__input name" placeholder="Username" />
                    </div>
                    <div class="login__row">
                        <svg class="login__icon pass svg-icon" viewBox="0 0 20 20">
                            <path d="M0,20 20,20 20,8 0,8z M10,13 10,16z M4,8 a6,8 0 0,1 12,0" />
                        </svg>
                            <input asp-for="Password" type="password" class="login__input pass" placeholder="Password" />
                    </div>
                    <button type="submit" class="login__submit">Sign in</button>
                </div>
            </form>
        </div>
    </div>
</div>
<!-- partial -->
